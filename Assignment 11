    #include <iostream>
    using namespace std;
     
    int main() {
    	int num1;
    	int num2;
    	int gcd;
     
    	cout << "Enter in 2 numbers: ";
    	cin >> num1 >> num2;
    	cout << endl;
     
    	for (int i = 1; i <= num1; i++)
    	{
    		if (num1 % i == 0 && num2 % i == 0)
    		{
    			gcd = i;
    		}
    	}
     
    	cout << "The greatest common divider is " << gcd << endl;
     
    	return 0;
    }
